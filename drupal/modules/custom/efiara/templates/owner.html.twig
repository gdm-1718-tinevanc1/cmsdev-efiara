{#
/**
 * @file owner.html.twig
 * Default theme implementation to present Owner data.
 *
 * This template is used when viewing Owner pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_owner()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('owner') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
